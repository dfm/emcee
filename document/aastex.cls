            \global\advance\@colht by\right@cor
        \fi
        \global\advance\@colht by\cor@height
        \global\cor@height=0pt
    \fi
\else
    \ifdim\c@colht>\@colht%
        \@tempdima\@colht%
        \ifdim\@colht>\z@%%
            \ifvoid\footins%
            \else%
                \vskip-\skip\footins%
            \fi%
            \splittopskip0pt%
            \setbox\my@outputbox\vbox{%
            \vspace*{\@colht}%%
            \global\botraggfalse%
            \vskip.7\baselineskip%
            \vsplit\@outputbox to \baselineskip%
            \unvbox\@outputbox}%
            \unvbox\my@outputbox%
